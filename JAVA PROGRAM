 <servlet-class>UserPass</servlet-class>
 </servlet>
 <servlet-mapping>
 <servlet-name>UserPass</servlet-name>
 <url-pattern>/servlet/UserPass</url-pattern>
 </servlet-mapping>
</web-app>
